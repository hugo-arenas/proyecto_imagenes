i=imread('cancer_celulas.png');
ig = rgb2gray(i);
%umbral=graythresh(ig);
ib=imbinarize(ig,0.3);
ib = 1 - ib;
%se = strel('disk',4);
%ie = imerode(ib,se);
ib5 = edge(ib,'Canny');
se = strel('disk',4);
ie = imerode(ib,se);
ie = imdilate(ib5,se);
ie = imerode(ie,se);
%ie = ib - ie;

figure, 
subplot(1,2,1), imshow(i), title('original'); 
titulo=['umbral Otsu = ' int2str(umbral*255)];
subplot(1,2,2), imshow(ie), title(titulo);

